 "html": "<div class=\"container\">\n    <div class=\"heading\">\n        <img src=\"/assets/frappe/images/GSOD%20LOGO.png\" class=\"img-responsive\" style=\"margin:auto;height:40px;\" />\n        <h3 style=\"text-align:center\">Gujarati Samaj of Detroit</h3>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"section-heading\">\n            <h4>Sponsor Details</h4>\n        </div>\n        <div class=\"col-xs-12 column-break\">\n            {% if doc.sponsor_type=='Guest' %}\n            <div class=\"row\">\n                <div class=\"col-xs-5\">\n                    <label>Name</label>\n                </div>\n                <div class=\"col-xs-7\">{{doc.sponsor_name}} {{doc.last_name}}</div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-5\">\n                    <label>Email</label>\n                </div>\n                <div class=\"col-xs-7\">{{doc.email}}</div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-5\">\n                    <label>Phone Number</label>\n                </div>\n                <div class=\"col-xs-7\">{{doc.phone}}</div>\n            </div>           \n            {% else %}\n            <div class=\"row\">\n                <div class=\"col-xs-5\">\n                    <label>Member Id</label>\n                </div>\n                <div class=\"col-xs-7\">{{doc.member}}</div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-5\">\n                    <label>Member Name</label>\n                </div>\n                <div class=\"col-xs-7\">{{doc.sponsor_name}} {{ doc.last_name }}</div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-5\">\n                    <label>Email</label>\n                </div>\n                <div class=\"col-xs-7\">{{doc.email}}</div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-5\">\n                    <label>Phone</label>\n                </div>\n                <div class=\"col-xs-7\">{{doc.phone}}</div>\n            </div>            \n            {% endif %}\n        </div>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"section-heading\">\n            <h4>Sponsorship Details</h4>\n        </div>\n        <div class=\"col-xs-12 column-break\">            \n            <div class=\"row\">\n                <div class=\"col-xs-5\">\n                    <label>Sponsorship Type</label>\n                </div>\n                <div class=\"col-xs-7\">{{doc.sponsorship_type}}</div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-5\">\n                    <label>Sponsorship Plan</label>\n                </div>\n                <div class=\"col-xs-7\">{{doc.sponsorship_plan}}</div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-5\">\n                    <label>Sponsorship Starts On</label>\n                </div>\n                <div class=\"col-xs-7\">{{doc.starts_on}}</div>\n            </div>    \n            \n            <div class=\"row\">\n                <div class=\"col-xs-5\">\n                    <label>Sponsorship Expires On</label>\n                </div>\n                <div class=\"col-xs-7\">{{doc.expires_on}}</div>\n            </div>\n        </div>\n    </div>\n    {% set payment=frappe.get_list('Payment Entries',fields=['*'],filters={'ref_id':doc.name}) %}\n    {% if payment %}\n    <div class=\"row section-break\">\n        <div class=\"section-heading\">\n            <h4>Payment Details</h4></div>\n        <div class=\"col-xs-12 column-break\">\n            \n            <div class=\"row\">\n                <div class=\"col-xs-5\">\n                    <label>Payment Date</label>\n                </div>\n                <div class=\"col-xs-7\">{{payment[0].payment_date}}</div>\n            </div>\n\t    {% if payment[0].transaction_id %}\n            <div class=\"row\">\n                <div class=\"col-xs-5\">\n                    <label>Transaction Id</label>\n                </div>\n                <div class=\"col-xs-7\">{{payment[0].transaction_id}}</div>\n            </div>\n            {% endif %}\n            <div class=\"row\">\n                <div class=\"col-xs-5\">\n                    <label>Amount</label>\n                </div>\n                <div class=\"col-xs-7\">$ {{payment[0].paid_amount}}</div>\n            </div>\n        </div>\n    </div>\n    {% endif %}\n</div>", 
