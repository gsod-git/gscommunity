  "html": "<div class=\"container\">\n<div class=\"heading\">\n<img src=\"/assets/frappe/images/GSOD%20LOGO.png\" class=\"img-responsive\" style=\"margin:auto;height:40px;\" />\n<h3 style=\"text-align:center\">Gujarati Samaj of Detroit</h3>\n<h5 style=\"text-align:center\">Team - {{doc.song_title}}</h5>\n</div>\n<div class=\"row section-break\">\n<div class=\"col-xs-6 column-break\">\n<div class=\"row\"><div class=\"col-xs-5\"><label>Title</label></div>\n<div class=\"col-xs-7\">{{doc.song_title}}</div></div>\n<div class=\"row\"><div class=\"col-xs-5\"><label>Event</label></div>\n<div class=\"col-xs-7\">{{doc.events}}</div></div>\n<div class=\"row\"><div class=\"col-xs-5\"><label>Competition</label></div>\n<div class=\"col-xs-7\">{{doc.competition}}</div></div>\n</div>\n<div class=\"col-xs-6 column-break\">\n<div class=\"row\"><div class=\"col-xs-5\"><label>Status</label></div>\n<div class=\"col-xs-7\">{{doc.status}}</div></div>\n<div class=\"row\"><div class=\"col-xs-5\"><label>Age Group</label></div>\n<div class=\"col-xs-7\">{{doc.age_group}}</div></div>\n</div>\n</div>\n<div class=\"row section-break\">\n<div class=\"section-heading\"><h4>Managing Team</h4></div>\n<div class=\"col-xs-12\">\n<table class=\"table table-bordered\">\n<thead>\n<tr>\n<th>sr</th><th>Member</th><th>Member Name</th><th>Status</th><th>Member Type</th><th>WhatsApp Number</th><th>Email</th>\n</tr>\n</thead>\n<tbody>\n{% if doc.table_11 %}{% for member in doc.table_11 %}\n<tr>\n<td>{{member.idx}}</td><td>{{member.member}}</td><td>{{member.member_name}}</td><td>{{member.active}}</td><td>{{member.member_type}}</td><td>{{member.whatsapp_number}}</td><td>{{member.email}}</td>\n</tr>\n{% endfor %}{% endif %}\n</tbody>\n</table>\n</div>\n</div>\n<div class=\"row section-break\">\n<div class=\"section-heading\"><h4>Participants</h4></div>\n<div class=\"col-xs-12\">\n<table class=\"table table-bordered\">\n<thead>\n<tr>\n<th>sr</th><th>Member</th><th>Member Name</th><th>Status</th><th>Prize Type</th>\n</tr>\n</thead>\n<tbody>\n{% if doc.table_7 %}{% for member in doc.table_7 %}\n<tr>\n<td>{{member.idx}}</td><td>{{member.member}}</td><td>{{member.member_name}}</td><td>{{member.active}}</td><td>{{member.prize_type}}</td>\n</tr>\n{% endfor %}{% endif %}\n</tbody>\n</table>\n</div>\n</div>\n<div class=\"row section-break\">\n<div class=\"section-heading\"><h4>Song</h4></div>\n<div class=\"col-xs-12 column-break\">\n<div class=\"row\"><div class=\"col-xs-3\"><label>Song</label></div>\n<div class=\"col-xs-9\">{{doc.song}}</div></div>\n<div class=\"row\"><div class=\"col-xs-3\"><label>Song Duration</label></div>\n<div class=\"col-xs-9\">{{doc.song_duration}}</div></div>\n</div>\n</div>\n{% if doc.table_15 %}\n<div class=\"row section-break\">\n<div class=\"section-heading\"><h4>Song Background</h4></div>\n<div class=\"col-xs-12\">\n<table class=\"table table-bordered\">\n<thead>\n<tr>\n<th>sr</th><th>From Time</th><th>To Time</th><th>Color / Color Code</th\n</tr>\n</thead>\n<tbody>\n{% for item in doc.table_15 %}\n<tr>\n<td>{{item.idx}}</td><td>{{item.from_time}}</td><td>{{item.to_time}}</td>{% if item.color %}<td>{{item.color}}</td>{% endif %}{% if item.color_code %}<td>{{item.color_code}}</td>{% endif %}\n</tr>\n{% endfor %}\n</tbody>\n</table>\n</div>\n</div>\n{% endif %}\n</div>", 
